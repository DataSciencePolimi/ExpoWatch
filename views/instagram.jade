body
  include includes/navbar.jade
  .content
    .row
      .col-md-10.social
        div(role="tabpanel")
          ul.nav.nav-tabs(role="tablist")
            li(role="presentation").active
              a(href="#instagram_abs" aria-controls="instagram_abs" role="tab" data-toggle="tab") Instagram Absolute
            li(role="presentation")
              a(href="#instagram_delta" aria-controls="instagram_delta" role="tab" data-toggle="tab") Instagram Increment
          .tab-content
            #instagram_abs.tab-pane.active(role="tabpanel")
            #instagram_delta.tab-pane(role="tabpanel")
    .row
      .col-md-10
        #photo


