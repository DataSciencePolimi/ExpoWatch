header
  link(rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css")
  link(rel="stylesheet" href="//cdn.jsdelivr.net/bootstrap.material-design/0.1.5/css/material-wfont.min.css")
  link(rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.1.5/css/ripples.min.css")
  link(rel="stylesheet" href="//cdn.jsdelivr.net/bootstrap.material-design/0.1.5/css/material.min.css")
  link(rel="stylesheet" href="style.css")
body
  include includes/navbar.jade
  .content
   .row
     .col-md-10.social
        div(role="tabpanel")
          ul.nav.nav-tabs(role="tablist")
            li(role="presentation").active
              a(href="#twitter_abs" aria-controls="twitter_abs" role="tab" data-toggle="tab") Twitter Absolute
            li(role="presentation")
              a(href="#twitter_delta" aria-controls="twitter_delta" role="tab" data-toggle="tab") Twitter Increment
        .tab-content
          #twitter_abs.tab-pane.active(role="tabpanel")
          #twitter_delta.tab-pane(role="tabpanel")




script(src="javascript/lib/jquery.min.js")
script(src="javascript/lib/bootstrap.min.js")
